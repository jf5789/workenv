[user]
	name = jf
	email = jf5789@gmail.com
[core]
	quotepath = false
	excludesfile = /Users/abc/.gitignore
	editor = vim
[alias]
	st = status -uno
	stu = status 
	cm = commit
	co = checkout
	br = branch
	mt = mergetool
	di = diff --no-ext-diff

[push]
	default = current
[merge]
	tool = extMerge
[mergetool "extMerge"]
	cmd = extMerge \"$BASE\" \"$LOCAL\" \"$REMOTE\" \"$MERGED\"
[mergetool]
	trustExitCode = false
[diff]
	external = extDiff
[filter "media"]
	clean = git-media-clean %f
	smudge = git-media-smudge %f
